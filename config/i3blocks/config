separator_block_width=20

[network-info]
command=~/.config/i3blocks/scripts/network-status.sh
interval=30
signal=1

[brightness]
command=~/.config/i3blocks/scripts/brightness.sh
interval=once
signal=1

[volume]
command=~/.config/i3blocks/scripts/volume.sh
interval=once
signal=1

[battery]
command=~/.config/i3blocks/scripts/battery.sh
interval=30

# Date and time
[time]
command=date '+%d-%m-%Y %H:%M:%S'
interval=1
